{
    "name": "nazarene/backend",
    "description": "Paquete para la estructura eclesiástica de la Iglesia del Nazareno América del Sur",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "David Huanca",
            "email": "dhuanca@samnaz.org"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Nazarene\\Backend\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nazarene\\Backend\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nazarene\\Backend\\NazareneServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 